{
  "name": "atx-urban-trails",
  "version": "1.0.2",
  "private": true,
  "dependencies": {
    "@turf/center-mean": "^6.0.1",
    "axios": "^0.18.0",
    "bootstrap": "4.3.1",
    "jquery": "^3.3.1",
    "leaflet": "1.4.0",
    "popper.js": "1.14.6",
    "prop-types": "^15.6.2",
    "react": "16.7.0",
    "react-dom": "16.7.0",
    "react-leaflet": "2.1.4"
  },
  "devDependencies": {
    "autoprefixer": "9.4.4",
    "axios-mock-adapter": "1.16.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "10.0.1",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.5",
    "babel-preset-react-app": "7.0.0",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "^2.1.2",
    "chalk": "2.4.2",
    "css-loader": "2.1.0",
    "cypress": "3.1.4",
    "dom-testing-library": "3.16.3",
    "dotenv": "6.2.0",
    "dotenv-expand": "4.2.0",
    "eslint": "5.12.0",
    "eslint-config-standard": "12.0.0",
    "eslint-config-standard-react": "7.0.2",
    "eslint-loader": "2.1.1",
    "eslint-plugin-chai-friendly": "^0.4.1",
    "eslint-plugin-cypress": "2.2.0",
    "eslint-plugin-flowtype": "3.2.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "22.1.2",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-node": "8.0.1",
    "eslint-plugin-promise": "4.0.1",
    "eslint-plugin-react": "7.12.3",
    "eslint-plugin-standard": "4.0.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "3.0.1",
    "fs-extra": "7.0.1",
    "html-webpack-plugin": "^4.0.0-beta.5",
    "jest": "23.6.0",
    "jest-dom": "3.0.0",
    "node-sass": "4.11.0",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "prettier": "1.15.3",
    "promise": "8.0.2",
    "raf": "3.4.1",
    "react-dev-utils": "7.0.1",
    "react-testing-library": "5.4.4",
    "resolve": "1.9.0",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "sw-precache-webpack-plugin": "0.11.5",
    "url-loader": "1.1.2",
    "webpack": "4.28.3",
    "webpack-dev-server": "3.1.14",
    "webpack-manifest-plugin": "2.0.4",
    "whatwg-fetch": "3.0.0"
  },
  "scripts": {
    "lint": "eslint './src/**/*.js' && eslint './tests/**/*.js'",
    "lint:fix": "eslint --fix './src/**/*.js' && eslint --fix './tests/**/*.js'",
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "jest --env=jsdom --detectOpenHandles",
    "test:cov": "jest --env=jsdom --coverage",
    "cy": "cypress open",
    "cy:all": "cypress run"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}",
      "!src/index.js",
      "!src/map/**",
      "!src/api/base.js",
      "!src/trails/urban-trails.js"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/tests/unit/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
